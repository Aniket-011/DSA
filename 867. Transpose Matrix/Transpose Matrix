class Solution {
    public int[][] transpose(int[][] matrix) {
         int  col = matrix.length;
         int row = matrix[0].length;
             int[][] ans = new int[row][col];
         
        for(int i=0; i<row; i++){
            for(int j=0; j<col; j++){
                ans[i][j] = matrix[j][i];
                }
                }
        return ans;
    }
   
}

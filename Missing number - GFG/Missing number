class Compute {
    
    public static int missingNumber(int A[], int N)
    {
         // Your code goes here
         
        int sumarr =0;
        int expectsum =0;
        for(int i=0; i<A.length; i++){
            sumarr += A[i];
        }
        expectsum= (N*(N+1))/2;
        int ans = expectsum - sumarr;
        return ans;
    }
}
